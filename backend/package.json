{
  "name": "restaurant-backend",
  "version": "1.0.0",
  "description": "Backend API pour application de gestion de restaurant",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "docker:build": "docker build -t restaurant-backend .",
    "docker:run": "docker run -p 5000:5000 restaurant-backend",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop restaurant-backend",
    "pm2:restart": "pm2 restart restaurant-backend",
    "pm2:logs": "pm2 logs restaurant-backend"
  },
  "keywords": [
    "restaurant",
    "api",
    "nodejs",
    "express",
    "sequelize",
    "socket.io",
    "redis"
  ],
  "author": "Votre Nom",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.2.0",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "moment": "^2.29.4",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.1",
    "sequelize-cli": "^6.6.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/votre-username/restaurant-backend.git"
  },
  "bugs": {
    "url": "https://github.com/votre-username/restaurant-backend/issues"
  },
  "homepage": "https://github.com/votre-username/restaurant-backend#readme"
}
